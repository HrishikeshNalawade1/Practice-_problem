using System;
namespace program5
{
 class Program5
 {
     static void Main(string[] args)
 {
     Console.WriteLine("enter the number ");
     int number = Convert.ToInt32(Console.ReadLine());
     int count = 0;
     if (number >= 1)
     {
        while (number != 0)
    {
       int rem = number % 10;
        if (rem == 3)
   {
      count++;
   }
     number = number / 10;
   }
 }

    if(count>0)
 {
    Console.WriteLine("True");
 }
 else
 {
    Console.WriteLine("False");
 }
 }
 }
}
